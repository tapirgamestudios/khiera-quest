{
    "compilerOptions": {
        "jsx": "react-jsx",
        "target": "ES2022",
        "lib": [
            "DOM",
            "ES2022"
        ],
        "module": "NodeNext",
        "outDir": "./build",
        "esModuleInterop": true,
        "noImplicitAny": true,
        "strictNullChecks": true,
        "skipLibCheck": true,
    },
    "include": [
        "./src/"
    ]
}